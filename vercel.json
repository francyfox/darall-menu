{
  "version": 2,
  "installCommand": "pnpm i",
  "buildCommand": "npx prisma generate && pnpm run build",
  "builds": [
    {
      "src": "./dist/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "./public/uploads/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist"
    }
  ]
}